$name?="nrf52840"

using sysbus
mach create $name
machine LoadPlatformDescription @platforms/boards/miv-board.repl

$bin?=@nrf52840_xxaa.elf

showAnalyzer uart

macro reset
"""
    sysbus LoadELF $bin
"""
runMacro $reset
